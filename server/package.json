{
  "name": "3d-models-pinterest-backend",
  "version": "1.0.0",
  "description": "Backend API for 3D models Pinterest-like app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:env": "cross-env NODE_ENV=test jest",
    "test:sequential": "jest --runInBand",
    "create-test-db": "ts-node src/tests/createTestDb.ts"
  },
  "keywords": [
    "3d",
    "models",
    "api",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.10",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
